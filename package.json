{
  "name": "@zheqi/eslint-config-monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "nr stub",
    "stub": "pnpm -r --parallel run stub",
    "lint": "eslint .",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag"
  },
  "keywords": [
    "eslint"
  ],
  "author": "zhe-qi",
  "license": "MIT",
  "devDependencies": {
    "@zheqi/eslint-config": "workspace:*",
    "@zheqi/eslint-plugin": "workspace:*",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6"
  }
}
